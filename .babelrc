{
	"env": {
		"c": {
			"presets": [
				["env", {
					"debug": true,
					"targets": {
						"browsers": ["last 2 versions", "ie > 8"]
					},
					"useBuiltIns": true
				}]
			],
		},
		"s": {
			"presets": [
				["env", {
					"debug": true,
					"targets": {
							"node": 7.7
						}
					}
				]
			],
		}
	},
	"plugins": [
		"dynamic-import-webpack",
		"transform-object-rest-spread",
		"transform-class-properties"
	]
}
